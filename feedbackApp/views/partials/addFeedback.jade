.col-md-9
  .col-md-10
    h1 {{title}}
    form(method='POST')
      .formfield
        .formprompt
        label.addFeedLabels Prospect Name
        br
        input#name(type='text', name='name', ng-model='feedback.name')
      .formfield
        .formprompt
        label.addFeedLabels Phone
        br
        input#phone(type='text', name='phone', ng-model='feedback.phone')
      .formfield
        .formprompt
        br
        input#status(type='radio', name='string', ng-model='feedback.status', value='Gave Code')
        label.addFeedLabels Gave Code
      .formfield
        .formprompt
        br
        input#status(type='radio', name='string', ng-model='feedback.status', value='Scheduled Showing')
        label.addFeedLabels Scheduled Showing
      .formfield
        .formprompt
        br
        input#status(type='radio', name='string', ng-model='feedback.status', value='Call Back If App Falls Through')
        label.addFeedLabels Call Back if App Falls Through
      .formfield
        .formprompt
        br
        input#status(type='radio', name='string', ng-model='feedback.status', value='Call Back to Show')
        label.addFeedLabels Call Back to Show
        br
        input.submitButton.addFeedBtn.btn.btn-default(type='submit', ng-click='addFeedback()')

