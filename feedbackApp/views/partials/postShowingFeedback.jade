.col-md-9
  .col-md-10
    h1 {{title}}
    form(method='POST')
      .formfield
        .formprompt
        label.postShowLabels Prospect Name
        br
        input#name(type='text', name='name', ng-model='showingPros.name')
      .formfield
        .formprompt
        label.postShowLabels Phone Number
        br
        input#name(type='text', name='phone', ng-model='showingPros.phone')
      .formfield
        .formprompt
        label.postShowLabels Comments About Property
        br
        input#name(type='text', name='comments', ng-model='showingPros.comments')
      input.submitPostShowButton.btn.btn-default(type='submit', ng-click='addPostShowFeedback()')

